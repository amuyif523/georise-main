{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.ts",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint .",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "test:coverage": "dotenv -e .env.test -- vitest run --coverage",
    "test:db:migrate": "dotenv -e .env.test -- prisma migrate deploy",
    "test:db:seed": "dotenv -e .env.test -- ts-node prisma/seed.test.ts",
    "test:db:reset": "npm run test:db:migrate && npm run test:db:seed",
    "seed": "npm run prisma-seed",
    "prisma-seed": "ts-node prisma/seed.ts",
    "simulate:responder": "node scripts/emitResponderLocation.js"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
